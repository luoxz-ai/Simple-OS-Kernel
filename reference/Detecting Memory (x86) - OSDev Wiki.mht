pan> <span class=3D"kw4">short</span> total<span class=3D"sy0">;</span>
    <span class=3D"kw4">unsigned</span> <span class=3D"kw4">char</span> =
lowmem<span class=3D"sy0">,</span> highmem<span class=3D"sy0">;</span>
